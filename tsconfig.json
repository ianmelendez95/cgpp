{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vitest/globals"],
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "dist"
  },
  "include": ["src"]
}
